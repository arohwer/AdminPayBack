<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>NCCS Innovation Lab | @ViewBag.Title</title>
    @*<link href="@Url.Content("~/Content/main.css")" rel="stylesheet" type="text/css" />*@
    @*<link rel='stylesheet' href="~/Content/main.css" />*@
    <link rel="shortcut icon" href="~/Images/icon/favicon.ico" type="image/x-icon">
    <link rel="icon" href="~/Images/icon/favicon.ico" type="image/x-icon">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?/json?&key=AIzaSyAqgNXE59IwEiUyj-X5sxw-pxWsIvYIVWw&libraries=places" async defer></script>
    @Styles.Render("/Content/main.css")

</head>
<body>
    <nav class='navbar-fixed-top navbar-gray'>
        <a href="javascript:void(0)" id='close-menu' class="closebtn">&times;</a>
        <img class='nav-icon' src="~/Images/icon/nclogo.png" id='nc-icon' />
        @RenderSection("Navigation")
    </nav>
    <div class="navbar-side">
        <span id="hamburger" onclick="">&#9776;</span>
        <img class="nav-icon-side" src="~/Images/icon/nclogo.png" id='nc-icon-side' />
    </div>
    <header></header>
    <div>
        @RenderBody()
    </div>
    <script src="~/Scripts/EventCalendarScript.js"></script>
</body>
</html>

